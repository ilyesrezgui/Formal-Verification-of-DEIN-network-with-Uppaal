//int nbbit=22; // represents the number of bits for a message. Article : Create Your Own Data and Energy Integrated Communication Network: A Brief Tutorial and a  Prototype System
int decoding_percentage= 100;// it should be chosen accoridngly to global-Esent since it represents a part of the emitted value.
double Energy_Harvested; //The percentage of energy to harvest if you're the requester.

bool W2H_Request=false;   
double capacity=2500;// to be varried from 1 to 250 W/KG and it should depend on whether it is a battery or a super capacitator, to be changed in mW or micro Watt
int sensing_value=10; // in milliwatt/cm² to be varried accordingly
int Threshold_for_sending=15; // represnts the threshhold for sending since sending data to hap also consumes energy for data encoding.
double WD_energy_lost =0;

int WD_receiving=4; 
clock time_to_receive;
clock WD_return; 
int period_for_WD=1; 

const double M_PI       = 3.141592653589793115997963468544185161590576171875; 
const double SPEED_OF_LIGHT = 299792458;

import "C:/Users/ilyes/OneDrive/Bureau/Ilyes REZGUI/My_Research/DEIN Network Formal Verification/Models/External_c++/libexternal.so" {
    int castToInt(double n);
};
 
// this function calulates lambda value represnting the wavelength
double calculateWavelength(double frequency) {
    return SPEED_OF_LIGHT / frequency;
}

// this will calculate the path loss L
double calculateFSPL(double distanceMeters, double frequencyHz) {
    // Calculate the FSPL in dB
    double fspl = 20 * log10(distanceMeters) + 20 * log10(frequencyHz) - 147.55;

    return fspl;
}

//  PT is the energy emitted by the HAP
//  GT and GR are the gain of the antenna in term of dbi 
//  frequency in HZ if the frequency for communication , in the RF interval 
//  d is the distance 

double calculateReceivedPower(double PT, double GT, double GR, double frequency_in_Hz, double d) {
    double lambda = calculateWavelength(frequency_in_Hz);
    double L_dB = calculateFSPL(d, frequency_in_Hz);
    double L = pow(10, L_dB / 10.0); // Convert FSPL from dB to linear scale

    // Calculate the received power using the Friis transmission equation
    double numerator = PT * GT * GR * pow(lambda, 2);
    double denominator = pow(4 * M_PI * d, 2) * L;
    double PR = numerator/denominator;
    return PR;
}


double verif_capacity(double x,int y){
// we want to analyse energy lost with repsect to distance  
double k=calculateReceivedPower(x,3.8,3.8,915e6,distance);      // x is the energy emission and it should be in microwattt
if (k+y&gt; capacity){
        //energy_lost=castToInt(energy_lost+(k+y-capacity));
        WD_energy_lost=WD_energy_lost+castToInt(k+y-capacity);
        return capacity; 
    }
    else {
        return k+y;}
}

int discrete_verif_capacity (double x,int y){
double res;
res=verif_capacity(x,y);
return castToInt(res);
}

